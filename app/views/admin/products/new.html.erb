<h1>商品新規登録</h1>
<%= form_with model: @product, local:true do |f| %>

<h4>商品画像</h4>
<%= f.attachment_field :product_image %>

<h4>商品名</h4>
<%= f.text_field :product_name %>

<h4>商品説明</h4>
<%= f.text_area :product_description %>

<h4>ジャンル</h4>


<h4>価格（税抜き）</h4>
<%= f.text_field :seles_price %>円

<h4>販売ステータス</h4>
<%= f.text_field :seles_status %>

<%= f.submit '新規登録' %>

<% end %>